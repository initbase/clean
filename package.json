{
  "name": "clean-ui",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "docs:generate": "cd docs && yarn && yarn generate",
    "bundle": "npx bundle-scss -m \"./src/**/theme.scss\" -d \"./dist/themes.scss\"",
    "scss": "node-sass \"./src\" -o \"./dist\"",
    "tailwind:build": "npx tailwind build ./dist/themes.scss -o ./dist/themes.css",
    "postbuild": "yarn bundle && yarn tailwind:build"
  },
  "devDependencies": {
    "bundle-scss": "^1.5.0"
  },
  "dependencies": {
    "autoprefixer": "^10.2.5",
    "css-loader": "^5.2.0",
    "cssnano": "^4.1.10",
    "node-sass": "^5.0.0",
    "postcss": "^8.2.9",
    "postcss-loader": "^5.2.0",
    "sass-loader": "^11.0.1",
    "style-loader": "^2.0.0",
    "tailwindcss": "^2.0.4",
    "webpack": "^5.30.0",
    "webpack-cli": "^4.6.0"
  }
}
